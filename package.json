{
  "name": "@femshima/djs-interaction",
  "version": "0.2.0",
  "description": "Framework for interactions of discord.js",
  "main": "dist/index.js",
  "license": "MIT",
  "exports": {
    ".": {
      "type": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "author": "femshima",
  "keywords": [
    "discord",
    "bot",
    "commands"
  ],
  "homepage": "https://github.com/femshima/djs-interaction",
  "bugs": {
    "url": "https://github.com/femshima/djs-interaction/issues"
  },
  "repository": "github:femshima/djs-interaction",
  "scripts": {
    "build": "tsc -d",
    "check": "tsc --noEmit",
    "dev": "ts-node sample/index.ts",
    "dev:watch": "ts-node-dev sample/index.ts",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "start": "node dist/index.js",
    "test": "jest --passWithNoTests",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "discord.js": "^14.3.0"
  },
  "devDependencies": {
    "@cm-ayf/readenv": "^1.1.0",
    "@types/jest": "^28.1.8",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.2"
  }
}
